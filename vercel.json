{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "env": {
    "API_KEY": "@api_key",
    "STABILITY_API_KEY": "@stability_api_key",
    "HF_API_KEY": "@hf_api_key"
  }
}
